[global]
rw=randwrite
blocksize=1M
ioengine=libaio
norandommap=1
direct=1
iodepth=32
iodepth_batch=16
iodepth_batch_complete=16
group_reporting=1
ramp_time=5
time_based=1
runtime=600s
randrepeat=0
directory={{ .Device.Mountpoint }}
unlink=0
disable_lat=0
disable_clat=0
disable_slat=0
numjobs=4
size=25g
filename_format=fiodata.$jobnum

[{{ .Name }}]
description="random 1M write peak bandwidth, 4 jobs"
write_lat_log={{ .FioLatLog }}
write_bw_log={{ .FioBWLog }}
write_iops_log={{ .FioIopsLog }}

