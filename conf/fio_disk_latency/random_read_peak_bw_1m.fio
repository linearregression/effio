[global]
rw=randread
blocksize=1M
ioengine=libaio
norandommap=1
direct=1
iodepth=32
iodepth_batch=16
iodepth_batch_complete=16
group_reporting=1
ramp_time=5
time_based=1
runtime=600s
randrepeat=0
size=100g
directory={{ .Device.Mountpoint }}
numjobs=1

[{{ .Name }}]
description="random 1M read peak bandwidth, 1 job"
write_bw_log={{ .FioBWLog }}
write_lat_log={{ .FioLatLog }}
write_iops_log={{ .FioIopsLog }}

