[global]
rw=randrw
rwmixread=25
rwmixwrite=75
blocksize=4096
ioengine=sync
norandommap=1
direct=1
iodepth=1
iodepth_batch=1
iodepth_batch_complete=1
group_reporting=1
ramp_time=5
time_based=1
runtime=600s
randrepeat=0
size=100g
directory=/mnt/effio/ioswitch_raijin_ssd
numjobs=1
unlink=1
disable_lat=0
disable_clat=1
disable_slat=1

[ioswitch_raijin_ssd-random_rw2575_latency_512]
description="random 4096b 25/75 read/write latency, 1 job"
write_lat_log=lat

